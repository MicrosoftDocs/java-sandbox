{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ],
        "src": "docs-ref-autogen",
        "dest": "java-sandbox"
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "java-sandbox/breadcrumb/toc.yml",
      "extendBreadcrumb": true
    },
    "fileMetadata": {},
    "template": [],
    "dest": "java-sandbox",
    "exportRawModel": true,
    "rawModelOutputFolder": "../_model",
    "exportViewModel": true,
    "viewModelOutputFolder": "../_viewmodel"
  }
}